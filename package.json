{
  "name": "padutility",
  "version": "1.0.0",
  "description": "Utility for puzzles and dragons, monster look up etc...",
  "author": {
    "name": "Matthew Homan"
  },
  "license": "MIT",
  "scripts": {
    "client": "cd client && npm run start",
    "server": "node server.js",
    "THISWILLAUTOREFRESHSERVERIFCHANGEDTOserverabove": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "dev:server": "cd client && npm build && cd .. && npm run start",
    "start-server": "node server.js",
    "start": "node server.js",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "dependencies": {
    "base-64": "^0.1.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "mongodb": "^3.2.2",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.9",
    "phantom": "^6.2.0",
    "request": "^2.88.0",
    "webpack": "4.19.1",
    "webpage": "^0.3.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matthewhoman/padutility.git"
  },
  "keywords": [
    "pad",
    "puzzlesanddragons"
  ]
}
